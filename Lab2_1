#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    double x, y;
    printf("Enter x:");
    scanf("%lf", &x);
 if( x>= -3 && x < 3){
 x = 4*sin(3*x)+3*cos(4*x);
 }

 else if ( x >= 10 && x < 20 ){
 x = 2*pow(x,2)-(5*x)+9;
 }

 else if ( x >= 100 && x < 1000 ){
 x = (log10(pow(x,2)))/2-3*x;
 }

 else{
  x = -(x/10)+1;
 }
    system("cls");
    printf("x = %.2lf", x);
    printf("\ny = %.2lf", y);

    return 0;
}
